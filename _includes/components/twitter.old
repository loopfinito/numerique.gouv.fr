<section id="tweets" class="padding-top-3">
  <h2 class="font-bold text-center margin-bottom-3 ">{{ page.titre-twitter }}</h2>
  <div class="grid-container">
    <div class="grid-x grid-margin-x">
      {% for twitter in page.twitters %}
      <div class="cell large-4 medium-6 small-12">
        {% include components/twitter.html nbTweet=twitter.nbTweets address=twitter.address %}
      </div>
      {% endfor %}
    </div>
  </div>
</section>
